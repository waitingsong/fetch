  "compilerOptions": {
    "declaration": false,
    "module": "commonjs",
    "newLine": "lf",
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "./",
    "pretty": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext"
  },
  "include": [
    "init-repo/**/*.ts"
  ],
  "exclude": [
  ]
}
