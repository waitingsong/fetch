{
  "name": "@waiting/egg-fetch",
  "author": "waiting",
  "version": "2.1.1",
  "description": "fetch for egg, midway",
  "eggPlugin": {
    "name": "fetch"
  },
  "keywords": [
    "midway",
    "egg",
    "eggPlugin",
    "egg-plugin",
    "fetch",
    "http",
    "cookies",
    "typescript",
    "ts"
  ],
  "bin": {},
  "browser": "",
  "es2015": "",
  "main": "./index.cjs.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "bugs": {
    "url": "https://github.com/waitingsong/rxxfetch/issues"
  },
  "homepage": "https://waitingsong.github.io/rxxfetch/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waitingsong/rxxfetch.git"
  },
  "license": "MIT",
  "dependencies": {
    "@waiting/shared-core": "3",
    "@waiting/shared-types": "3",
    "rxjs": "^6.5.2",
    "rxxfetch": "^2.1.1"
  },
  "devDependencies": {
    "cross-env": "7",
    "egg": "^2.23.0"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "files": [
    "config",
    "lib",
    "agent.js",
    "agent.d.ts",
    "app.js",
    "app.d.ts",
    "index.js",
    "index.cjs.js",
    "index.cjs.js.map",
    "index.d.ts"
  ],
  "scripts": {
    "build": "npm run tsc && npm run rp",
    "clean": "rm -rf dist/* .vscode/.tsbuildinfo .vscode/.tsbuildinfo.* ",
    "cov": "cross-env TS_NODE_PROJECT=test/tsconfig.json nyc mocha",
    "lint": "eslint --fix --cache {src,test}/**/*.ts",
    "lint:nofix": "eslint --cache {src,test}/**/*.ts",
    "rp": "rollup -c rollup.config.js",
    "rp:w": "npm run tsc:w | rollup -wc rollup.config.js",
    "test": "cross-env TS_NODE_PROJECT=test/tsconfig.json mocha",
    "tsc": "tsc -p tsconfig.json",
    "tsc:w": "tsc -p tsconfig.json -w"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      ".githooks",
      "dist",
      "node_modules*",
      "src/bin",
      "src/domain.ts",
      "src/interface.ts",
      "test*",
      "**/node_modules*",
      "**/*.d.ts",
      "**/*.js"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "json",
      "html"
    ],
    "all": true
  }
}
