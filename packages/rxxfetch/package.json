{
  "name": "rxxfetch",
  "author": "waiting",
  "version": "3.0.0",
  "description": "Observable HTTP fetch API for browser and Node.js. Handle 302/303 redirect correctly on Node.js",
  "keywords": [
    "rx",
    "rxjs",
    "rxjs6",
    "http",
    "fetch",
    "nodejs",
    "Node.js",
    "Generics",
    "typescript",
    "Observable",
    "AbortController",
    "303"
  ],
  "bin": {},
  "browser": "./dist/rxxfetch.esm.js",
  "es2015": "./dist/rxxfetch.esm.js",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/waitingsong/rxxfetch/issues"
  },
  "homepage": "https://waitingsong.github.io/rxxfetch/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waitingsong/rxxfetch.git"
  },
  "license": "MIT",
  "dependencies": {
    "@types/node-fetch": "^2.1.1",
    "@types/qs": "^6.5.1",
    "@waiting/shared-types": "5",
    "abortcontroller-polyfill": "^1.3.0",
    "form-data": "^3.0.0",
    "node-fetch": "^2.6.0",
    "qs": "^6.7.0",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@waiting/shared-core": "5",
    "cross-env": "7"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run tsc && npm run rp",
    "clean": "rm -rf dist/* .vscode/.tsbuildinfo .vscode/.tsbuildinfo.* ",
    "cov": "cross-env TS_NODE_PROJECT=test/tsconfig.json nyc mocha",
    "lint": "eslint --fix --cache {src,test}/**/*.ts",
    "lint:nofix": "eslint --cache {src,test}/**/*.ts",
    "rp": "rollup -c rollup.config.js",
    "rp:w": "npm run tsc:w | rollup -wc rollup.config.js",
    "test": "cross-env TS_NODE_PROJECT=test/tsconfig.json mocha",
    "test:browser": "cd .config && npm run browser && cd ..",
    "browser:debug": "cd .config && npm run debug && cd ..",
    "tsc": "tsc -p tsconfig.json",
    "tsc:w": "tsc -p tsconfig.json -w"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      ".githooks",
      "dist",
      "node_modules*",
      "src/bin",
      "src/domain.ts",
      "src/interface.ts",
      "test*",
      "**/node_modules*",
      "**/*.d.ts",
      "**/*.js"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "json",
      "html"
    ],
    "all": true
  }
}
